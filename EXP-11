str1 <- "The quick quick brown fox jumps over the lazy dog."
print("Original vector (string):")
print(str1)
print("Unique elements of the said vector:")
print(unique(strsplit(str1, split = " ")[[1]]))
nums <- c(1, 2, 2, 3, 4, 4, 5, 6)
print("Original vector (number):")
print(nums)
print("Unique elements of the said vector:")
print(unique(nums))
